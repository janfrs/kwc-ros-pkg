<?xml version="1.0" encoding="UTF-8"?>
<!-- name of the robot-->
<robot name="pr2">

	<link name="left_shoulder"><!-- link specifying the shoulder of the robot -->

		<parent> torso</parent>

		<rpy> 0 0 0 </rpy> <!-- rotation of a local coordinate frame attached to the link with respect to a global coordinate frame -->

		<joint type="revolute" name="joint_left_shoulder"> 
			<axis> 0 0 1 </axis> <!-- direction of the joint in a global coordinate frame -->
			<anchor>0.25 0.1 0.3  </anchor> <!-- point on the joint relative to the parent's anchor in a global coordinate frame -->
			<limit> -1 1 </limit>
			<calibration>1.5 -1 </calibration> <!-- Calibration stop/flag indicating location and then direction -->
		</joint>

		<inertial>
			<mass> 10.0  </mass>
			<com> 0.5 0.1 2 </com> <!-- position of the center of mass with respect to the link's own anchor in a local coordinate frame -->
			<inertia>Ixx Iyy Izz Ixy Iyz Ixz </inertia>
		</inertial> 

		<visual>
			<xyz>0 0 0 </xyz> <!-- location defined with respect to the link origin in a local coordinate frame -->
			<rpy>0.0 0.0 0.0 </rpy> <!-- All angles always in radians, yaw about Z axis, pitch about the Y axis and roll about the X axis -->
			<material>Gazebo/Grey</material>
			<geometry type="box" name="visual_left_shoulder"><!-- geometry specified using a simple geometric object -->
				<size> </size> 
			</geometry>

			<geometry type="mesh" name="visual_left_shoulder">
				<filename/><!-- mesh specified using an obj file --> 
			</geometry>
		</visual> 

		<collision>
			<xyz> 0 0 0 </xyz> <!-- location defined with respect to the link origin in a local coordinate frame -->
			<rpy>0.0 0.0 0.0 </rpy> <!-- All angles always in radians, yaw about Z axis, pitch about the Y axis and roll about the X axis -->
			<material/> 
			<geometry type="box" name="collision_left_shoulder"><!-- geometry specified using a simple geometric object -->
				<size> </size> 
			</geometry>

			<geometry type="mesh" name="collision_left_shoulder">
				<filename/><!-- mesh specified using an obj file --> 
			</geometry>
		</collision>

	</link>









	<link name="base"><!-- link specifying the base of the robot -->

		<parent> world</parent>

		<rpy> 0 0 0 </rpy> <!-- rotation of a local coordinate frame attached to the link with respect to a global coordinate frame -->

		<joint type="floating" name="joint_base"> 
			<anchor>0.25 0.1 0.3  </anchor> <!-- Initial position of the base in a global coordinate frame -->
		</joint>

		<inertial>
			<mass> 80.0  </mass>
			<com> 0 0 0 </com> <!-- position of the center of mass with respect to the link's own anchor in a local coordinate frame -->
			<inertia>Ixx Iyy Izz Ixy Iyz Ixz </inertia>
		</inertial> 

		<visual>
			<xyz> 0 0 0 </xyz> <!-- location defined with respect to the link origin in a local coordinate frame -->
			<rpy>0.0 0.0 0.0 </rpy> <!-- All angles always in radians, yaw about Z axis, pitch about the Y axis and roll about the X axis -->
			<material>Gazebo/Grey</material>
			<geometry type="box" name="visual_base"><!-- geometry specified using a simple geometric object -->
				<size> </size> 
			</geometry>

			<geometry type="mesh" name="visual_base">
				<filename/><!-- mesh specified using an obj file --> 
			</geometry>
		</visual> 

		<collision>
			<xyz> 0 0 0 </xyz> <!-- location defined with respect to the link origin in a local coordinate frame -->
			<rpy>0.0 0.0 0.0 </rpy> <!-- All angles always in radians, yaw about Z axis, pitch about the Y axis and roll about the X axis -->
			<material/> 
			<geometry type="box" name="collision_base"><!-- geometry specified using a simple geometric object -->
				<size> </size> 
			</geometry>

			<geometry type="mesh" name="collision_base">
				<filename/><!-- mesh specified using an obj file --> 
			</geometry>
		</collision>

	</link>


	<sensor name="left_forearm_camera" type="camera"> 

		<calibration> filename </calibration>
		<parent>left_forearm</parent>
		<xyz> 10 1 10 </xyz> <!-- position of a local coordinate frame attached to the sensor with respect to the parent link's coordinate frame -->
		<rpy> 0 0 0 </rpy> <!-- rotation of a local coordinate frame attached to the sensor with respect to the parent link's coordinate frame -->
		<inertial>
			<mass> 10.0  </mass>
			<com> 0.5 0.1 2 </com> <!-- position of the center of mass with respect to the sensor's local coordinate frame -->
			<inertia>Ixx Iyy Izz Ixy Iyz Ixz </inertia>
		</inertial> 
		<visual>
			<xyz> 0 0 0</xyz> <!-- location defined with respect to the link origin in the sensor's local coordinate frame -->
			<rpy>0.0 0.0 0.0 </rpy> <!-- All angles always in radians, yaw about Z axis, pitch about the Y axis and roll about the X axis -->
			<material>Gazebo/Grey</material>
			<geometry type="box" name="visual_left_forearm_camera"><!-- geometry specified using a simple geometric object -->
				<size> </size> 
			</geometry>

			<geometry type="mesh" name="visual_left_forearm_camera">
				<filename/><!-- mesh specified using an obj file --> 
			</geometry>
		</visual> 
		<collision>
			<xyz> 0 0 0 </xyz> <!-- location defined with respect to the link origin in the sensor's local coordinate frame -->
			<rpy>0.0 0.0 0.0 </rpy> <!-- All angles always in radians, yaw about Z axis, pitch about the Y axis and roll about the X axis -->
			<material/> 
			<geometry type="box" name="collision_left_forearm_camera"><!-- geometry specified using a simple geometric object -->
				<size> </size> 
			</geometry>

			<geometry type="mesh" name="collision_left_forearm_camera">
				<filename/><!-- mesh specified using an obj file --> 
			</geometry>
		</collision>
	</sensor>



</robot>