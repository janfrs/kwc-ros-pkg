  </joint:hinge>
  <joint:hinge name="fixed_neck_body_3">
    <body1>torso_body</body1>
    <body2>neck_body</body2>
    <anchor>torso_body</anchor>
    <axis>  0.0   0.0   1.0 </axis>
  </joint:hinge>

