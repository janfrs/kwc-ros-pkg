      <!--size>.20 .38 .14</size-->
      <size>0.01 0.01 0.01</size>
      <mass>0.01</mass>
      <visual>
        <mesh>unit_box</mesh>
        <!--size>.20 .38 .14</size-->
        <size>0.01 0.01 0.01</size>
        <material>Gazebo/Black</material>
      </visual>
    </geom:box>
  </body:box>
