<launch>
  <master auto="start"/>
  <include file="$(find executive_trex_pr2)/cfg/launch_stage.xml" />
  <param name="/trex/input_file" value="nav.cfg"/>
  <param name="/trex/path" value="$(find executive_trex_pr2)/cfg:$(find executive_trex_pr2)/wpc.1"/>
  <param name="/trex/time_limit" value="45"/>
  <param name="/trex/log_dir" value="$(find executive_trex_pr2)/wpc.1"/>
  <param name="/trex/play_back" value="0"/>
  <test test-name="executive_trex_wpc.1" pkg="executive_trex_pr2" type="trexfast" time-limit="60"/>
</launch>
