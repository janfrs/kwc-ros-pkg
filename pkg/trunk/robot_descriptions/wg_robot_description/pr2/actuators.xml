<robot name="pr2">

  <joint name="joint_shoulder_pan_left">
    <actuator name="actuator_shoulder_pan_left_1">
      <motor>MAXON</motor>
      <ip> IP_ADDRESS </ip> 
      <port> 0 </port>
      <reduction>10</reduction>
      <polymap>-1 -0.2 -0.5 </polymap>
    </actuator> 
    
