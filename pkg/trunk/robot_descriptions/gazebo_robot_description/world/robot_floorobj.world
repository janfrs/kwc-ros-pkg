  <!-- The "desk"-->
  <model:physical name="desk1_model">
    <xyz>   2.25  -0.21  -0.10</xyz>
    <rpy>   0.0    0.0    0.00</rpy>
    <body:box name="desk1_legs_body">
      <geom:box name="desk1_legs_geom">
        <kp>100000000.0</kp>
        <kd>1.0</kd>
        <xyz>   0.0    0.0    0.4</xyz>
        <mesh>default</mesh>
        <size>0.5 1.0  0.6</size>
        <mass> 10.0</mass>
        <visual>
          <size> 0.5 1.0 0.6</size>
          <material>Gazebo/Rocky</material>
          <mesh>unit_box</mesh>
