<launch>
  <include file="init.machine" />
  <include file="$(find caster_life_test)/send_description.launch" />

  <!-- pr2_etherCAT -->
  <node machine="xenomai_root" pkg="pr2_etherCAT" type="pr2_etherCAT" args="-i rteth0 -x /robotdesc/pr2"/>

  <!-- Calibration -->
	<!--
  <node pkg="mechanism_bringup" type="calibrate.py"
        args="$(find caster_life_test)/cal.xml"
        output="screen" />
				-->

  <!-- Power board -->
  <node pkg="pr2_power_board" type="power_node"  respawn="true"/>

  <!-- Runtime Diagnostics Logging -->
  <node pkg="rosrecord" type="rosrecord"
        args="-f /u/mwise/runtime_logs/testcart6_runtime.bag /diagnostics" />

</launch>
